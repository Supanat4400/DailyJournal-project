<%- include("partials/header") -%> 
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="newTitle">
        <label>Title</label>
        <input type="text" name = "postTitle" class="form-control" autofocus autocomplete="off">  
        <label>Post</label>
        <textarea class="form-control" id="postArea" name="postBody" rows="5" cols="30"></textarea>

    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>


<%- include("partials/footer") -%> 